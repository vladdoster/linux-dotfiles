cf       ${XDG_CONFIG_HOME:-$HOME/.config}
cac      ${XDG_CACHE_HOME:-$HOME/.cache}
h        ${XDG_CACHE_HOME:-$HOME/}
d        ${XDG_CACHE_HOME:-$HOME/downloads}
gh       ${XDG_CACHE_HOME:-$HOME/github}
