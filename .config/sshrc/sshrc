#!/bin/bash

#-> sshrc
export REMOTE_SSHRC_CONFIGS_DIR=$REMOTE_SSHRC_DIR/sshrc.d
source $REMOTE_SSHRC_CONFIGS_DIR/functions.sh
source $REMOTE_SSHRC_CONFIGS_DIR/.bashrc

#-> correct username
if [ "$USER" != "$(whoami)" ]; then
  USER=$(whoami)
fi

#-> tmux
if which tmux >/dev/null 2>&1; then
  tmuxrc-create-or-attach-session
fi
